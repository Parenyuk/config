{
  "name": "config",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint && pnpm lint:type-check && pnpm lint:staged",
    "lint:prettier-fix": "prettier . --write",
    "lint:eslint-fix": "eslint . --fix",
    "lint:staged": "pretty-quick --staged",
    "lint:type-check": "tsc --noEmit",
    "fix": "pnpm lint:prettier-fix && pnpm lint:eslint-fix && pnpm lint:staged",
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky"
  },
  "dependencies": {
    "eslint-plugin-next": "^0.0.0",
    "husky": "^9.1.7",
    "next": "15.1.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.1.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "lint-staged": "^15.2.11",
    "postcss": "^8",
    "prettier": "3.4.2",
    "prettier-plugin-classnames": "^0.7.5",
    "prettier-plugin-merge": "^0.7.2",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "pretty-quick": "^4.0.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown",
    "**/*.{ts,tsx}": [
      "eslint --fix"
    ]
  },
  "packageManager": "pnpm@9.15.0+sha224.2bd0703201ffa752d0a3828b086a87c3729ac17e205beef8dda33024"
}
